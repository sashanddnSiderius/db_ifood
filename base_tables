create table User (
	id int not null auto_increment primary key,
    date_created string,
    status int,
    name string,
    email string,
    password string,
    phone string,
    cpf string
),

create table Restaurant (
	id int not null auto_increment primary key,
    date_created string,
    status int,
    name string,
    description string,
    phone string,
    rate string,
    cnpj string
),

create table Restaurant_category (
	id int not null auto_increment primary key,
    data_created string,
    status int,
    name string,
    description string
)

create table Address (
	id int not null auto_increment primary key,
    date_created string,
    status int,
    street string,
    district string,
    cep string,
    number string,
    complement string,
    default int
),

create table Product (
	id int not null auto_increment primary key,
    date_created string,
    status int,
    name string,
    price int,
    description string
),

create table Product_category (
	id int not null auto_increment primary key,
    data_created string,
    status int,
    name string,
    description string,
),

create table Product_add (
	id int not null auto_increment primary key,
    data_created string,
    status int,
    name string,
    description string,
    price int,
),

